@using Library.Models;
@{
    Layout="_Layout";
}
<div class="header">
    <h1 class="title">@Model["book"].GetTitle()</h1>
    <hr>
</div>
<h2> By
@foreach (Author author in Model["authors"])
{
    @author.GetName() 
}</h2>

<p>Copies</p>
@foreach (int copy in Model["copies"])
{
    <p>@copy</p>
}

<p><a href="/books">Go back</a></p>